# Configuración de Selenium
SELENIUM_TIMEOUT_SHORT=8
SELENIUM_TIMEOUT_MEDIUM=14
SELENIUM_TIMEOUT_LONG=18
SELENIUM_HEADLESS=true

# Rutas por defecto
DEFAULT_INPUT_PATH=/home/usuario/Documents/PDFs
DEFAULT_OUTPUT_PATH=/home/usuario/Documents/Resultados

# Configuración de procesamiento
MAX_WORKERS=4
ZOOM_LEVELS=2,3,4,5,6
DPI_LEVELS=150,200,300

# Logging
LOG_LEVEL=ERROR
LOG_FILE=process_log.txt
